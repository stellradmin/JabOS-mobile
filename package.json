{
  "name": "jabos-mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --go",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:compatibility": "jest tests/compatibility-algorithms.test.ts",
    "test:integration": "jest tests/match-flow-integration.test.tsx",
    "test:sql": "psql $DATABASE_URL -f ../stellr-backend/tests/sql-functions.test.sql",
    "test:error-boundaries": "jest tests/error-boundary.test.tsx",
    "test:accessibility": "jest tests/accessibility.test.tsx",
    "test:algorithms": "npm run test:compatibility && npm run test:integration && npm run test:error-boundaries && npm run test:accessibility",
    "test:feature-validation": "jest __tests__/feature-matrix/phase-validation.test.ts",
    "test:security-comprehensive": "jest __tests__/security/comprehensive-security-validation.test.ts",
    "test:phases": "npm run test:feature-validation && npm run test:security-comprehensive",
    "test:production-ready": "npm run test && npm run test:phases && npm run test:accessibility && npm run lint && npm run type-check",
    "test:jest": "jest --watchAll",
    "test:onboarding": "node tests/test-onboarding-flow.js",
    "test:matching": "node tests/test-matching-system.js",
    "test:messaging": "node tests/test-messaging-system.js",
    "test:signup": "node tests/test-signup-functionality.js",
    "test:profile": "node tests/test-profile-data-flow.js",
    "generate:test-data": "node tests/generate-test-data.js",
    "create:test-users": "node scripts/create-test-users.js",
    "clear:test-users": "node scripts/clear-test-users.js",
    "seed:messaging:test": "node scripts/seed-messaging-test-data.js",
    "clear:messaging:test": "node scripts/clear-messaging-test-data.js",
    "lint": "expo lint",
    "type-check": "tsc -p tsconfig.app.json --noEmit",
    "deploy:check": "./scripts/deployment/pre-deploy-checklist.sh",
    "deploy:production": "./scripts/deployment/deploy-production.sh",
    "deploy:ios": "./scripts/deployment/deploy-production.sh ios",
    "deploy:android": "./scripts/deployment/deploy-production.sh android",
    "test:week7": "ts-node src/tests/week7-test-orchestrator.ts",
    "test:week7:performance": "ts-node src/tests/week7-test-orchestrator.ts --performance-only",
    "test:week7:integration": "ts-node src/tests/week7-test-orchestrator.ts --integration-only",
    "test:week7:mobile": "ts-node src/tests/week7-test-orchestrator.ts --mobile-only",
    "test:week7:full": "npm run test:week7",
    "bundle:analyze": "EXPO_NO_TELEMETRY=1 npx expo export --platform ios --output-dir .expo-dist && npx source-map-explorer '.expo-dist/**/*.js' '.expo-dist/**/*.map' --html bundle-report.html",
    "bundle:size": "npm run bundle:analyze && echo 'Bundle analysis complete. Check bundle-report.html'",
    "bundle:stats": "EXPO_NO_TELEMETRY=1 npx expo export --platform ios --output-dir .expo-dist && npx @bundle-stats/cli .expo-dist --out-dir .artifacts/bundle-stats",
    "security:scan": "gitleaks detect -v -s . -c .gitleaks.toml --redact --no-banner",
    "security:scan:history": "gitleaks detect -v -s . -c .gitleaks.toml --log-opts='--all' --redact --no-banner",
    "security:audit": "npm audit --audit-level=high && npm run security:scan",
    "security:validate": "./scripts/security-validation.sh",
    "postinstall": "patch-package"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "./jest.setup.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|@sentry|native-base|react-native-svg|@react-native-google-signin|react-native-purchases|react-native-ssl-pinning)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.expo/",
      "/tests/"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "components/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.test.{ts,tsx}",
      "!src/**/__tests__/**",
      "!tests/**/*",
      "!**/__mocks__/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@invertase/react-native-apple-authentication": "^2.4.1",
    "@miblanchard/react-native-slider": "^2.6.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "^8.4.4",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-community/slider": "5.0.1",
    "@react-native-google-signin/google-signin": "^13.3.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@sentry/react-native": "^7.4.0",
    "@shopify/flash-list": "2.0.2",
    "@supabase/supabase-js": "^2.50.2",
    "autoprefixer": "^10.4.20",
    "circular-natal-horoscope-js": "^1.1.0",
    "expo": "^54.0.2",
    "expo-apple-authentication": "~8.0.7",
    "expo-application": "~7.0.7",
    "expo-atlas": "^0.4.3",
    "expo-battery": "~10.0.7",
    "expo-blur": "~15.0.7",
    "expo-build-properties": "~1.0.8",
    "expo-constants": "~18.0.8",
    "expo-crypto": "~15.0.7",
    "expo-dev-client": "~6.0.12",
    "expo-device": "~8.0.7",
    "expo-file-system": "~19.0.12",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-local-authentication": "~17.0.7",
    "expo-localization": "~17.0.7",
    "expo-location": "~19.0.7",
    "expo-notifications": "~0.32.11",
    "expo-router": "~6.0.1",
    "expo-secure-store": "~15.0.7",
    "expo-sharing": "~14.0.7",
    "expo-splash-screen": "~31.0.9",
    "expo-sqlite": "~16.0.8",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-updates": "~29.0.12",
    "expo-web-browser": "~15.0.7",
    "isomorphic-dompurify": "^2.26.0",
    "lightningcss": "^1.29.2",
    "lucide-react-native": "^0.525.0",
    "moment": "^2.30.1",
    "nativewind": "^4.1.23",
    "postcss": "^8.5.3",
    "posthog-react-native": "^4.2.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-chart-kit": "^6.12.0",
    "react-native-css-interop": "0.1.22",
    "react-native-device-info": "^14.0.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-persona": "^2.21.1",
    "react-native-purchases": "^9.5.4",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-ssl-pinning": "^1.6.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.21.0",
    "react-native-webcrypto": "1.0.0-alpha.3",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "tailwindcss": "^3.4.17",
    "tempo-devtools": "^2.0.102"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@faker-js/faker": "^9.0.3",
    "@react-native-community/cli": "^18.0.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^25.1.0",
    "@types/react": "~19.1.10",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.19.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~54.0.10",
    "patch-package": "^8.0.0",
    "prettier": "^3.2.5",
    "react-test-renderer": "19.0.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
